{
  "manifest_version": 2,
  "name": "AnkiMelon",
  "version": "1.0",
  "description": "Automatically synchronizes your Animelon vocabulary with Anki!",
  "content_scripts": [
    {
      "matches": ["*://*.animelon.com/*"],
      "js": ["ankimelon.js"]
    }
  ],
  "background": {
      "scripts": [
          "webRequests.js"
      ]
  },
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "*://animelon.com/"
  ]
}
